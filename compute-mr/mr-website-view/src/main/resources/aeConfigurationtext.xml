    </entry>
    <entry key="inbound">
      <ref local="inboundBucketMetrics"/>
    </entry>
    <entry key="event">
      <ref local="eventBucketMetrics"/>
    </entry>
    <entry key="order">
      <ref local="orderBucketMetrics"/>
    </entry>
  </util:map>

