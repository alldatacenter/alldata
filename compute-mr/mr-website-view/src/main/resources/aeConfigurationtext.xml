    <value>times</value>
  </util:set>

  <util:set id="inboundBucketMetrics">
    <value>uv</value>
    <value>visit</value>
    <value>bounce_number</value>
  </util:set>

  <util:set id="depthBucketMetrics">
    <value>view_depth_of_user</value>
