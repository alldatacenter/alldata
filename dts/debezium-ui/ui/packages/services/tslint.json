{
  "extends": ["tslint:recommended", "tslint-react", "tslint-eslint-rules", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "**/node_modules/**/*.ts"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "interface-name": false,
    "interface-over-type-literal": false,
    "no-console": false
  }
}
