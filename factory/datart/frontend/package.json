    "react-draggable": "^4.4.3",
    "react-grid-layout": "^1.2.4",
    "react-helmet-async": "1.2.2",
    "react-hotkeys-hook": "^3.4.0",
    "react-i18next": "11.15.3",
    "react-monaco-editor": "0.46.0",
    "react-quill": "1.3.5",
